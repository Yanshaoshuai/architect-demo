<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yan.architect.architectdemo.mapper.UserMapper">
  <resultMap id="userMap" type="com.yan.architect.architectdemo.pojo.User">
    <id property="id" jdbcType="BIGINT" column="id"/>
    <result property="account" jdbcType="VARCHAR" column="account"/>
    <result property="password" jdbcType="VARCHAR" column="password"/>
    <result property="name" jdbcType="VARCHAR" column="name"/>
    <result property="sex" jdbcType="TINYINT" column="sex"/>
    <result property="birthday" jdbcType="DATE" column="birthday"/>
    <result property="roleId" jdbcType="BIGINT" column="role_id"/>
    <result property="createTime" jdbcType="TIMESTAMP" column="create_time"/>
    <result property="updateTime" jdbcType="TIMESTAMP" column="update_time"/>
  </resultMap>
</mapper>
